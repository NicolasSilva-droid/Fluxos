{
  "name": "cy-fluxos",
  "version": "1.0.0",
  "description": "Cypress Fluxos",
  "homepage": "https://github.com/NicolasSilva-droid/Fluxos#readme",
  "bugs": {
    "url": "https://github.com/NicolasSilva-droid/Fluxos/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasSilva-droid/Fluxos.git"
  },
  "license": "ISC",
  "author": "Aluno Ebac",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "start": "npx serverest",
    "cy:run": "npx cypress run",
    "cy:report": "npx mochawesome-merge ./mochawesome-report/mochawesome*.json > mochawesome-reporter",
    "cy:run-ci": "start-server-and-test start http://localhost:3000/api-doc test",
    "testcy": "npx cypress run --reporter mochawesome --reporter-options reportDir=\"cypress/results\",overwrite=false,html=false,json=true && npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json && npx marge mochawesome.json",
    "test:allure": "npx cypress run --env allure=true && npm run allure:generate",
    "allure:open": "npx allure serve",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "sorry:cypress": "CYPRESS_API_URL=\"http://localhost:1234/\" npx cy2 run --record --key EBAC --parallel --ci-build-id `date +%s`"
  },
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.19.5",
    "allure-commandline": "^2.13.8",
    "cy2": "^1.3.0",
    "cypress": "^15.8.1",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.0.1"
  },
  "devDependencies": {
    "start-server-and-test": "^2.1.3"
  }
}
